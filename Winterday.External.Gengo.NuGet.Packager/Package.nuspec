<?xml version="1.0"?>
<package >
  <metadata>
    <id>gengo-dotnet</id>
    <version>0.9.4</version>
    <title>Gengo.com Client Library</title>
    <authors>Jarl Erik Schmidt</authors>
    <owners></owners>
    <description>
      This library provides a client for the Gengo.com REST API, allowing for management of translation jobs within .NET programs.
      Account keys necessary to use the client can be obtained through the account page on Gengo.com.
      This client is unofficial and provided under the terms of the MIT license.
    </description>
    <releaseNotes>
      Includes a series of improvements contributed by github.com/arzul: Improvements to duplicate detection, added support for custom_data in
      Quote payloads, and max_chars in Job payloads.
    </releaseNotes>
    <summary>
      .NET client library for Gengo.com API.
    </summary>
    <language>en-US</language>
    <projectUrl>https://github.com/cryovat/gengo-dotnet</projectUrl>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://raw2.github.com/cryovat/gengo-dotnet/master/COPYING</licenseUrl>
    <copyright>Copyright 2014</copyright>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="Newtonsoft.Json" version="6.0.1" />
      </group>
    </dependencies>
    <references></references>
    <tags></tags>
  </metadata>
  <files>
    <file src="lib\" target="lib" />
    <file src="tools\" target="tools" />
    <file src="content\" target="content" />
  </files>
</package>